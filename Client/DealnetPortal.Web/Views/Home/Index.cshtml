@using System.Configuration;

@{
    ViewBag.Title = Resources.HomePage;
}



<div id="home-body">
    <div class="margin-block">
        <div class="row">
            @*<div class="new-notification-wp">
                    <div class="new-notification">
                        <span class="new-notification__left-icon-block">
                            <i class="notification-alert-ico"></i>
                        </span>
                        <span class="new-notification__message">Due to planned maintenance, our site will be unavailable starting from 16/03/2017 at 10 PM EDT until 17/03/2017 at 5 AM EDT.</span>
                        <i class="close-white-ico"></i>
                    </div>
                </div>*@
            @if(ConfigurationManager.AppSettings["SwitchPopUpOnOff"] == "true")
            {
                @Html.Action("GetMaintanenceBanner")
            }

            <div class="new-notification-wp">
                <div class="new-notification-blank">
                    <span class="new-notification-blank__text">
                        On this page you can see the most recent contracts those were created and also leads, filled contracts from customer link and your drafts. Here we will post important information, e.g. notification about planned maintenance and some special offer for you.
                    </span>
                </div>
            </div>
        </div>
    </div>
    <!-- ko with: table-->
    <div class="margin-block">
        <div class="row">
            <div class="filters">
                <div class="filters-head">
                    <div class="filters-head__title">@Resources.MyDeals</div>
                    <div class="filters-head__learn-more">
                        <i class="lamp-ico"></i>
                        <a>@Resources.LearnMoreAboutThisPage</a>
                    </div>
                    <div class="filters-head__clear-btn new-btn-grey">
                        <span>Clear</span>
                    </div>
                    <div class="filters-head__save-btn">
                        <div class="new-btn-green">
                            <span>Save</span>
                        </div>
                        <span class="save-btn-status">
                            <i class="checked-ico"></i>(For next time)
                        </span>
                    </div>
                    <div class="filters-head__divider"></div>
                    <div class="filters-head__filter-btn new-btn-red-filter">
                        <i class="filter-ico filter-ico--red"></i>
                        <span>Filters</span>
                    </div>
                    <div class="filters-head__sort-btn new-sort-btn-red">
                        <i class="sort-button-red-ico"></i>
                        <span>Sort</span>
                    </div>
                </div>
                <div class="filters-advanced homepage-mod">
                    <div class="filters-advanced__type new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">Deal type</label>
                            <select name="project-type" id="project-type" class="custom-select"></select>
                        </div>
                    </div>
                    <div class="filters-advanced__status new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">Deal status</label>
                            <select name="project-type" id="project-type" class="custom-select"
                                    data-bind="options:statusOptions, value:status"></select>
                        </div>
                    </div>

                    <div class="filters-advanced__date-from new-input-group">
                        <div class="custom-text-input-group icon">
                            <input type="text" required data-bind="value:dateFrom">
                            <label>Date from</label>
                            <span class="icon-calendar"></span>
                        </div>
                    </div>

                    <div class="filters-advanced__date-to new-input-group">
                        <div class="custom-text-input-group icon">
                            <input type="text" required data-bind="value:dateTo">
                            <label>Date to</label>
                            <span class="icon-calendar"></span>
                        </div>
                    </div>

                    <div class="filters-advanced__created-by new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">Type of project</label>
                            <select name="project-type" id="project-type" class="custom-select" data-bind="options:equipmentOptions, value: equipment">
                                <option value="">Air conditioner</option>
                                <option value="">Air filtration</option>
                                <option value="">Air handler</option>
                            </select>
                        </div>
                    </div>

                    <div class="filters-advanced__sales-rep new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">Sales rep</label>
                            <select name="project-type" id="project-type" class="custom-select" data-bind="options:salesRepOptions, salesRep">
                                <option value="">Air conditioner</option>
                                <option value="">Air filtration</option>
                                <option value="">Air handler</option>
                            </select>
                        </div>
                    </div>

                    <div class="filters-advanced__filters-cloud">
                        <div class="filters-advanced__contract-number">Contract number - ascending<i class="cross-ico"></i></div>
                        <div class="filters-advanced__deal-status">Deal status - <i class="info-ico info-ico--red"></i> to <i class="info-ico info-ico--grey"></i> <i class="cross-ico"></i></div>
                        <div class="filters-advanced__e-sign">E-sign status - <i class="sign-ico sign-ico--red"></i> to <i class="sign-ico sign-ico--grey"></i> <i class="cross-ico"></i></div>
                        <div class="filters-advanced__applicant-name">Applicant name from Z to A<i class="cross-ico"></i></div>
                        <div class="filters-advanced__last-activity">Last activity — latest<i class="cross-ico"></i></div>
                        <div class="filters-advanced__days-left">Days left from ... to 1<i class="cross-ico"></i></div>
                    </div>

                    <div class="filters-advanced__clear-btn new-btn-grey"><span>Clear</span></div>
                    <div class="filters-advanced__apply-btn new-btn-solid-red"><span>Apply</span></div>
                </div>
            </div>
        </div>
    </div>

