    <div class="margin-block" data-bind="with: pager">
        <div class="row">
            <div class="new-pagination-wp">
                <div class="new-pagination">
                    <a data-bind="click: goToPreviousPage">
                        <i class="pagination-left-ico"></i>
                    </a>
                    <ul class="new-pagination__pages">
                        <li data-bind="foreach: allPages,
                            class:{ active: $parent.pageIndex() == ($data.pageNumber -1) },
                            click: $parent.moveToPage">
                            <a data-bind="text: $data.pageNumber"></a>
                        </li>
                        @*<li class="active"><a>2</a></li>
                            <li><a>...</a></li>
                            <li><a>10</a></li>
                            <li><a>11</a></li>*@
                    </ul>
                    <a data-bind="click: goToNextPage">
                        <i class="pagination-right-ico"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>

