@using System.Configuration;

@{
    ViewBag.Title = Resources.HomePage;
}



<div id="home-body">
    <div class="margin-block">
        <div class="row">
            @if(ConfigurationManager.AppSettings["SwitchPopUpOnOff"] == "true")
            {
                @Html.Action("GetMaintanenceBanner")
            }

            <div class="new-notification-wp">
                <div class="new-notification-blank">
                    <span class="new-notification-blank__text">
                        On this page you can see the most recent contracts those were created and also leads, filled contracts from customer link and your drafts. Here we will post important information, e.g. notification about planned maintenance and some special offer for you.
                    </span>
                </div>
            </div>
        </div>
    </div>
    <!-- ko with: table-->
    <div class="margin-block">
        <div class="row">
            <div class="filters">
                <div class="filters-head">
                    <div class="filters-head__title">@Resources.MyDeals</div>
                    <div class="filters-head__learn-more">
                        <i class="lamp-ico"></i>
                        <a>@Resources.LearnMoreAboutThisPage</a>
                    </div>
                    <div class="filters-head__clear-btn new-btn-grey">
                        <span>@Resources.Clear</span>
                    </div>
                    <div class="filters-head__save-btn">
                        <div class="new-btn-green">
                            <span>@Resources.Save</span>
                        </div>
                        <span class="save-btn-status">
                            <i class="checked-ico"></i>(@Resources.ForNextTime)
                        </span>
                    </div>
                    <div class="filters-head__divider"></div>
                    <div class="filters-head__filter-btn new-btn-red-filter">
                        <i class="filter-ico filter-ico--red"></i>
                        <span>@Resources.Filters</span>
                    </div>
                    <div class="filters-head__sort-btn new-sort-btn-red">
                        <i class="sort-button-red-ico"></i>
                        <span>@Resources.Sort</span>
                    </div>
                </div>
                <div class="filters-advanced homepage-mod">
                    <div class="filters-advanced__type new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">@Resources.DealType</label>
                            <select name="project-type" id="project-type" class="custom-select"></select>
                        </div>
                    </div>
                    <div class="filters-advanced__status new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">@Resources.DealStatus</label>
                            <select name="project-type" id="project-type" class="custom-select"
                                    data-bind="options:statusOptions, value:status"></select>
                        </div>
                    </div>

                    <div class="filters-advanced__date-from new-input-group">
                        <div class="custom-text-input-group icon">
                            <input type="text" required data-bind="value:dateFrom">
                            <label>@Resources.DateFrom</label>
                            <span class="icon-calendar"></span>
                        </div>
                    </div>

                    <div class="filters-advanced__date-to new-input-group">
                        <div class="custom-text-input-group icon">
                            <input type="text" required data-bind="value:dateTo">
                            <label>@Resources.DateTo</label>
                            <span class="icon-calendar"></span>
                        </div>
                    </div>

                    <div class="filters-advanced__created-by new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">@Resources.TypeOfProject</label>
                            <select name="project-type" id="project-type" class="custom-select" data-bind="options:equipmentOptions, value: equipment"></select>
                        </div>
                    </div>

                    <div class="filters-advanced__sales-rep new-input-group">
                        <div class="custom-select-wrap">
                            <label for="project-type">@Resources.SalesRep</label>
                            <select name="project-type" id="project-type" class="custom-select" data-bind="options:salesRepOptions, salesRep"></select>
                        </div>
                    </div>

                    <div class="filters-advanced__filters-cloud">
                        <div class="filters-advanced__contract-number">@Resources.ContractNumber -  ascending<i class="cross-ico"></i></div>
                        <div class="filters-advanced__deal-status">@Resources.DealStatus - <i class="info-ico info-ico--red"></i> to <i class="info-ico info-ico--grey"></i> <i class="cross-ico"></i></div>
                        <div class="filters-advanced__e-sign">@Resources.EsignStatus - <i class="sign-ico sign-ico--red"></i> to <i class="sign-ico sign-ico--grey"></i> <i class="cross-ico"></i></div>
                        <div class="filters-advanced__applicant-name">@Resources.ApplicantNameZtoA<i class="cross-ico"></i></div>
                        <div class="filters-advanced__last-activity">@Resources.LastActivity — latest<i class="cross-ico"></i></div>
                        <div class="filters-advanced__days-left">@Resources.CreditExpiryDesc<i class="cross-ico"></i></div>
                    </div>

                    <div class="filters-advanced__clear-btn new-btn-grey">
                        <span>@Resources.Clear</span>
                    </div>
                    <div class="filters-advanced__apply-btn new-btn-solid-red">
                        <span>@Resources.Apply</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="margin-block">
        <div class="row">

            <div class="main-table">
                <div class="main-table__block main-table__block--header">
                    <div class="main-table__block__row">
                        <div class="main-table__block__row__cell checkbox-cell">
                            <label class="customized-checkbox-icon">
                                <input id="checkBox" type="checkbox" />
                                <span class="customized-checkbox-icon__image"></span>
                            </label>
                        </div>
                        <div class="main-table__block__row__cell contract-number">
                            <span>@Resources.ContractNumber</span>
                            <span class="filter-ico filter-ico--asc">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell deal-status">
                            <span class="icon"><i class="info-ico info-ico--green"></i></span>
                            <span>@Resources.DealStatus</span>
                        </div>
                        <div class="main-table__block__row__cell e-sign">
                            <span>@Resources.Esign</span>
                        </div>
                        <div class="main-table__block__row__cell applicant-name">
                            <span>@Resources.ApplicantName</span>
                            <span class="filter-ico filter-ico--desc">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell last-activity">
                            <span>@Resources.LastActivity</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell days-left">
                            <span>@Resources.CreditApprovalExpiry</span>
                            <span class="filter-ico filter-ico--asc">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell loan-amount">
                            <span>@Resources.LoanAmount</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell program-option">
                            <span>@Resources.ProgramOption</span>
                        </div>
                        <div class="main-table__block__row__cell term">
                            <span>@Resources.Term</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell amort">
                            <span>@Resources.Amort</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell payment-amount">
                            <span>@Resources.Payment</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell funded-amount">
                            <span>@Resources.FundedAmount</span>
                            <span class="filter-ico filter-ico--default">
                                <i class="sort-arrow-top"></i><i class="sort-arrow-bottom"></i>
                            </span>
                        </div>
                        <div class="main-table__block__row__cell project-desc">
                            <span>@Resources.ProjectDescription</span>
                        </div>
                        <div class="main-table__block__row__cell address">
                            <span>@Resources.Address</span>
                        </div>
                        <div class="main-table__block__row__cell sales-rep">
                            <span>@Resources.SalesRep</span>
                        </div>
                        <div class="main-table__block__row__cell entered-by">
                            <span>@Resources.EnteredBy</span>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-notes">
                            <span>@Resources.Notes</span>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-lead">
                            <span>@Resources.Lead</span>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-gear">
                            <span></span>
                        </div>
                    </div>
                </div>

                <!-- ko foreach: pager.pagedList -->
                <div class="main-table__block main-table__block--body">
                    <div class="main-table__block__row">
                        <div class="main-table__block__row__cell checkbox-cell">
                            <label class="customized-checkbox-icon" data-bind="if: !$data.IsInternal && $data.Id != 0">
                                <input id="checkBox" type="checkbox" data-bind="checked: $data.isSelected" />
                                <span class="customized-checkbox-icon__image"></span>
                            </label>
                        </div>
                        <div class="main-table__block__row__cell contract-number">
                            <!-- ko if: $data.Id == 0-->
                            <span data-bind="text: $data.TransactionId"></span>
                            <!-- /ko -->
                            <!-- ko ifNot: $data.Id == 0 -->
                            <a data-bind="attr: { href:$parent.editUrl($data.Id) }, text: $data.TransactionId "></a>
                            <!-- /ko -->
                        </div>
                        <div class="main-table__block__row__cell deal-status">
                            <span class="icon">
                                <i data-bind="attr:{ class:'info-ico info-ico--'+ ($data.StatusColor||'grey')}"></i>
                            </span>
                            <span data-bind="text: $data.LocalizedStatus"></span>
                        </div>
                        <div class="main-table__block__row__cell e-sign">
                            <!-- ko if: $data.EsignStatus -->
                            <i data-bind="attr:{class:'sign-ico sign-ico--' +$data.SignatureStatusColor}"></i>
                            <span data-bind="text: $data.EsignStatus"></span>
                            <!-- /ko -->
                        </div>
                        <div class="main-table__block__row__cell applicant-name">
                            <span data-bind="text: $data.CustomerName"></span>
                        </div>
                        <div class="main-table__block__row__cell last-activity">
                            <span data-bind="text: $data.Date"></span>
                        </div>
                        <div class="main-table__block__row__cell days-left">
                            <span data-bind="text: $data.CreditExpiry"></span>
                        </div>
                        <div class="main-table__block__row__cell loan-amount">
                            <span data-bind="text: $data.Value"></span>
                        </div>
                        <div class="main-table__block__row__cell program-option">
                            <span data-bind="text: $data.Program"></span>
                        </div>
                        <div class="main-table__block__row__cell term">
                            <span data-bind="text: $data.LoanTerm"></span>
                        </div>
                        <div class="main-table__block__row__cell amort">
                            <span data-bind="text: $data.AmortTerm"></span>
                        </div>
                        <div class="main-table__block__row__cell payment-amount">
                            <span data-bind="text: $data.Payment"></span>
                        </div>
                        <div class="main-table__block__row__cell funded-amount">
                            <span data-bind="text: $data.LoanAmount"></span>
                        </div>
                        <div class="main-table__block__row__cell project-desc">
                            <span data-bind="text: $data.Equipment"></span>
                        </div>
                        <div class="main-table__block__row__cell address">
                            <span data-bind="text: $data.Address"></span>
                        </div>
                        <div class="main-table__block__row__cell sales-rep">
                            <span data-bind="text: $data.SalesRep"></span>
                        </div>
                        <div class="main-table__block__row__cell entered-by">
                            <span data-bind="text: $data.EnteredBy"></span>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-notes">
                            <i class="notes-ico notes-ico--grey"></i>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-lead">
                            <i data-bind="css:{'check-mark-ico':$data.Lead}" ></i>
                        </div>
                        <div class="main-table__block__row__cell icon-cell icon-cell-gear">
                            <i class="gear-ico"></i>
                            <span class="show-details">
                                <i class="sort-arrow-bottom sort-arrow-bottom--green"></i>
                            </span>
                            <div class="table-row-settings-popup">
                                <i class="cross-ico"></i>
                                <div class="table-row-settings-popup__edit">
                                    <i class="edit-contract-ico"></i>
                                    <span>
                                        <a data-bind="attr:{href:$parent.editUrl($data.Id)}">@Resources.Edit</a>
                                    </span>
                                </div>
                                <div class="table-row-settings-popup__export">
                                    <i class="excel-ico"></i>
                                    <span>
                                        <a>@Resources.ExportToExcel</a>
                                    </span>
                                </div>
                                <div class="table-row-settings-popup__help">
                                    <i class="ask-help-ico"></i>
                                    <span>
                                        <a>@Resources.AskForHelp</a>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="main-table__block__row">
                        <div class="table-row-details opened">
                            <div class="table-row-details__left">

                                <div class="table-row-details__left__last-activity">
                                    <span>@Resources.LastActivity</span>
                                    <span class="filter-ico filter-ico--asc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Date"></div>

                                <div class="table-row-details__left__days-left">
                                    <span>@Resources.CreditApprovalExpiry</span>
                                    <span class="filter-ico filter-ico--desc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.CreditExpiry"></div>

                                <div class="table-row-details__left__loan-amount">
                                    <span>@Resources.LoanAmount</span>
                                    <span class="filter-ico filter-ico--default">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Value"></div>

                                <div class="table-row-details__left__program-option">
                                    <span>@Resources.ProgramOption</span>
                                    <span class="filter-ico filter-ico--default">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Program"></div>

                                <div class="table-row-details__left__term">
                                    <span>@Resources.Term</span>
                                    <span class="filter-ico filter-ico--asc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.LoanTerm"></div>

                                <div class="table-row-details__left__amort">
                                    <span>@Resources.Amort</span>
                                    <span class="filter-ico filter-ico--desc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.AmortTerm"></div>

                                <div class="table-row-details__left__payment-amount">
                                    <span>@Resources.Payment</span>
                                    <span class="filter-ico filter-ico--desc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Payment"></div>

                                <div class="table-row-details__left__project-desc">
                                    <span>@Resources.ProjectDescription</span>
                                    <span class="filter-ico filter-ico--desc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Equipment"></div>

                                <div class="table-row-details__left__funded-amount">
                                    <span>@Resources.FundedAmount</span>
                                    <span class="filter-ico filter-ico--desc">
                                        <i class="sort-arrow-top"></i>
                                        <i class="sort-arrow-bottom"></i>
                                    </span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.FundedAmount"></div>

                            </div>
                            <div class="table-row-details__right">

                                <div class="table-row-details__left__address">
                                    <span>@Resources.Address</span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.Address"></div>

                                <div class="table-row-details__left__sales-rep">
                                    <span>@Resources.SalesRep</span>
                                </div>
                                <div class="table-row-details__left__value" data-bind="text: SalesRep"></div>

                                <div class="table-row-details__left__entered-by">
                                    <span>@Resources.EnteredBy</span>

                                </div>
                                <div class="table-row-details__left__value" data-bind="text: $data.EnteredBy"></div>

                                <div class="table-row-details__left__notes">
                                    <span>@Resources.Notes</span>
                                </div>
                                <div class="table-row-details__left__value">
                                    <i class="notes-ico notes-ico--grey"></i>
                                </div>

                                <div class="table-row-details__left__lead">
                                    <span>Lead</span>

                                </div>
                                <div class="table-row-details__left__value">
                                    <i class="check-mark-ico"></i>
                                </div>

                                <div class="table-row-details__left__gear">
                                    <span><i class="gear-ico"></i></span>

                                </div>
                                <div class="table-row-details__left__value"></div>

                            </div>
                        </div>
                    </div>
                </div>
                <!-- /ko -->
                <div class="main-table__block main-table__block--total">
                    <div class="main-table__block__row">
                        <div class="main-table__block__row__grand-total">
                            @Resources.GrandTotal
                        </div>
                        <div class="main-table__block__row__grand-total-value">
                            <span class="main-table__block__row__grand-total-value__total-container">$ <span data-bind="text: selectedTotal"></span></span>
                            <span>$ <span data-bind="text: grandTotal"></span></span>
                        </div>
                        <div class="main-table__block__row__entries-number new-input-group">
                            <div class="custom-select-wrap">
                                <label for="entries-number">Numbers of entries</label>
                                <select name="entries-number" 
                                        id="entries-number" 
                                        class="custom-select" 
                                        data-bind="options: pager.pageSizes, value:pager.pageSize"></select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="margin-block" data-bind="with: pager">
        <div class="row">
            <div class="new-pagination-wp">
                <div class="new-pagination">
                    <a data-bind="click: goToPreviousPage">
                        <i class="pagination-left-ico"></i>
                    </a>
                    <ul class="new-pagination__pages" data-bind="foreach: allPages">
                        <li data-bind="css: { active: $parent.pageIndex() == $data }">
                            <a data-bind="text: $data + 1, click: $parent.moveToPage"></a>
                        </li>
                    </ul>
                    <a data-bind="click: goToNextPage">
                        <i class="pagination-right-ico"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>

    <!-- /ko -->
    <div class="how-can-we-help-popup">
        <i class="cross-ico"></i>
        <span class="how-can-we-help-popup__title">How can we help?</span>
        <span class="how-can-we-help-popup__legend">Please fill this form and we will answer you with best solution.</span>
        <div class="how-can-we-help-popup__block">
            <div class="how-can-we-help-popup__block__info">
                <span>Dealer name</span>
                <span>utilecredit</span>
                <span>Your name</span>
                <span>utilecredit</span>
                <span>Deal number</span>
                <span>12345</span>
            </div>
            <div class="how-can-we-help-popup__block__checkbox new-checkbox-green">
                <input id="checkBox" type="checkbox" /><span>I have another name</span>
            </div>
            <div class="how-can-we-help-popup__block__your-name new-input-group">
                <div class="custom-text-input-group">
                    <input type="text" name="yourName" required>
                    <label>Your name</label>
                </div>
            </div>
        </div>
        <div class="how-can-we-help-popup__block">
            <div class="how-can-we-help-popup__block__theme new-input-group">
                <div class="custom-select-wrap">
                    <label for="theme">Please choose the theme</label>
                    <select name="theme" id="theme" class="custom-select">
                        <option value="">Credit docs</option>
                        <option value="">Credit docs</option>
                        <option value="">Credit docs</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="how-can-we-help-popup__block">
            <textarea class="how-can-we-help-popup__block__description"></textarea>
        </div>
        <div class="how-can-we-help-popup__block">
            <div class="how-can-we-help-popup__block__response-way new-input-group">
                <div class="custom-select-wrap">
                    <label for="response-way">Best way to get back to you</label>
                    <select name="response-way" id="response-way" class="custom-select">
                        <option value="">Alternative e-mail</option>
                        <option value="">Alternative e-mail</option>
                        <option value="">Alternative e-mail</option>
                    </select>
                </div>
            </div>
            <div class="how-can-we-help-popup__block__alternative-email new-input-group">
                <div class="custom-text-input-group">
                    <input type="text" name="alternativeEmail" required>
                    <label>Alternative e-mail</label>
                </div>
            </div>
        </div>
        <div class="how-can-we-help-popup__block">
            <div class="how-can-we-help-popup__block__buttons">
                <div class="new-btn-grey"><span>Cancel</span></div>
                <div class="new-btn-solid-green"><span>Send</span></div>
            </div>
        </div>
    </div>



    <div class="signature-status-popup">
        <i class="cross-ico"></i>
        <span class="signature-status-popup__title">Digital signature status</span>
        <span class="signature-status-popup__legend">Invites for digital signing of the contract were sent successfully - waiting for signatures.</span>

        <div class="signature-status-popup__block signature-status-popup__block--success">
            <div class="signature-status-popup__block__name">
                <span>Borrower</span>
                <span>John Domes</span>
            </div>
            <div class="signature-status-popup__block__email new-input-group">
                <div class="custom-text-input-group">
                    <input type="text" name="email" required>
                    <label>Email</label>
                </div>
            </div>
            <div class="signature-status-popup__block__status"><i class="signature-status-popup__block__status-ico"></i></div>
            <div class="signature-status-popup__block__signature">
                <span>Waiting for signature</span>
                <span>(10/08/2017 10:30 AM)</span>
            </div>
            <span class="signature-status-popup__block__link"></span>
