                        <div class="form-group table-length-filter"></div>
                    </div>
                    <div class="col-sm-6 col-md-2">
                        <button class="btn btn-green min-w-150 block-button filter-button">@Resources.Filter</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<table id="work-items-table" class="display responsive table-reports" width="100%">
