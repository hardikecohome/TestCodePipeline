<div class="rental-successful-page">
<div class="row">
    <div class="col-xs-12 col-sm-8 col-md-8 col-lg-5 message-block">
        <div class="text-center">
            <svg class="icon icon-successfully" width="70" height="70" viewBox="0 0 44 70">
                <use xlink:href="~/Content/images/sprite/sprite.svg#icon-successfully"></use>
            </svg>
        </div>
        <div class="message-header text-center">Rental Agreement submitted successfully!</div>
        <div class="message-desc">Thank you for completing the rental agreement. You will receive an email shortly at <a href="mailto:@ViewBag.HomeOwnerEmail">@ViewBag.HomeOwnerEmail</a> containing a copy of the completed agreement.</div>

        <div class="text-center">
            <a href="@Url.Action("BasicInfo")" class="btn dealnet-button dealnet-success-button wide-button">
                Create another application for this home owner
            </a>
        </div>
    </div>
</div>
</div>
