<div class="customer-success-page">
    <div class="customer-success-page-inner">
        <div class="text-center">
            <div class="icon-hold">
                <svg aria-hidden="true" class="icon icon-decline-circle"><use xlink:href="@Url.Content("~/Content/images/sprite/sprite.svg#icon-decline-circle")"></use></svg>
            </div>
            <div class="customer-success-page-content">
                <p>
                    <strong>@Resources.UnableToProcessApplicationAutomatically</strong>
                </p>
                <p class="customer-decline-address">
                    @Resources.InCaseQuestionsContactSupport<br>
                    <span class="text-lowercase">@Resources.Phone:</span> 1-866-382-7468 <br>
                    <span class="text-lowercase">@Resources.Email:</span> <a class="link-color-inherit" href="mailto: info@ecohomefinancial.com">info@ecohomefinancial.com</a>
                </p>
            </div>
            <div class="btn-container">
                <a href="@Url.Action("MyClients", "MortgageBroker")" class="btn dealnet-button dealnet-link-button">My Clients</a>
            </div>

            <div class="btn-container">
                <a href="@Url.Action("NewClient", "MortgageBroker")" class="btn dealnet-button dealnet-success-button">Add Another Client</a>
            </div>
        </div>
    </div>
</div>