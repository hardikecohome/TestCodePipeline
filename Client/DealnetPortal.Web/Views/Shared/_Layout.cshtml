<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title - DealNet</title>
    <link href="https://fonts.googleapis.com/css?family=Lato:300,300i,400,400i,700,700i,900" rel="stylesheet"  type='text/css'>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/libs")
    @Scripts.Render("~/bundles/dealnet")
    <link rel="stylesheet/less" type="text/css" href="~/Content/less/main.less">
    <script src="//cdnjs.cloudflare.com/ajax/libs/less.js/2.7.1/less.min.js"></script>
</head>
<body class="@(User.Identity.IsAuthenticated ? "authorized" : "no-authorized")">
<svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
    <symbol id="icon-edit" viewBox="0 0 20 20">
        <path d="M12.52 2.134c.552-.954 1.767-1.283 2.73-.727.957.552 1.285 1.774.735 2.727l-7.003 12.13-3.464-2 7.003-12.13zM5.02 15.13l3.464 2-3.732 2.463.268-4.464zm7-12.125l3.464 2-.5.866-3.464-2 .5-.865z" fill="#989897" fill-rule="evenodd"/>
    </symbol>
    <symbol id="icon-eye" viewBox="0 0 20 20">
        <path d="M10 15.015c5.523 0 10-5.015 10-5.015s-4.477-4.98-10-4.98S0 10 0 10s4.477 5.015 10 5.015zM10 14c2.21 0 4-1.79 4-4s-1.79-4-4-4-4 1.79-4 4 1.79 4 4 4zm0-1c1.657 0 3-1.343 3-3s-1.343-3-3-3-3 1.343-3 3 1.343 3 3 3z" fill="#989897" fill-rule="evenodd"/>
    </symbol>
    <symbol id="icon-scan-licence" viewBox="0 0 20 20">
        <path fill="#FFF" fill-rule="evenodd" d="M4 3.98h2.063l.915-2.014h6L14.02 3.98h2.983c1.103 0 1.997.898 1.997 2.008V15.99c0 1.11-.893 2.01-1.995 2.01H2.995C1.893 18 1 17.102 1 15.99V5.99c0-.74.404-1.39 1-1.737V3h2v.98zM3 15.99C3 16 17.005 16 17.005 16 17 16 17 5.988 17 5.988c0-.008-2.98-.008-2.98-.008h-1.216l-.56-1.08-1.042-2.013 1.775 1.08h-6L8.8 2.793l-.916 2.013L7.35 5.98H3.002c.007 0 0 10.01 0 10.01zM6 11c0-2.21 1.79-4 4-4s4 1.79 4 4-1.79 4-4 4-4-1.79-4-4zm6 0c0-1.105-.895-2-2-2s-2 .895-2 2 .895 2 2 2 2-.895 2-2z"/>
    </symbol>
    <symbol id="icon-remove" viewBox="0 0 20 20">
        <path fill="#CACACA" fill-rule="evenodd" d="M9.997 8.593l-2.12-2.12L6.45 7.898l2.12 2.122-2.11 2.11 1.424 1.422 2.11-2.11 2.097 2.1 1.43-1.428-2.097-2.097 2.123-2.122-1.424-1.423L10 8.592zM10 18c4.418 0 8-3.582 8-8s-3.582-8-8-8-8 3.582-8 8 3.582 8 8 8z"/>
    </symbol>
    <symbol id="icon-calendar" viewBox="0 0 20 20">
        <path fill="#979797" fill-rule="evenodd" d="M4 14.99c0 .557.452 1.01 1.006 1.01h9.988C15.55 16 16 15.55 16 14.99V7.01C16 6.452 15.548 6 14.994 6H5.006C4.45 6 4 6.45 4 7.01v7.98zM5.006 5h9.988C16.102 5 17 5.902 17 7.01v7.98c0 1.11-.897 2.01-2.006 2.01H5.006C3.898 17 3 16.098 3 14.99V7.01C3 5.9 3.897 5 5.006 5zM7 5.99C7 6.003 6.998 6 7 6V4.01C7 3.997 7.002 4 7 4v1.99zM7 3c.552 0 1 .443 1 1.01v1.98C8 6.55 7.556 7 7 7c-.552 0-1-.443-1-1.01V4.01C6 3.45 6.444 3 7 3zm-1 8h8v1H6v-1zm7-5.01c0 .012-.002.01 0 .01V4.01c0-.012.002-.01 0-.01v1.99zM6 8h8v1H6V8zm6 6h2v1H6v-1h2V8h1v6h2V8h1v6zm1-11c.552 0 1 .443 1 1.01v1.98C14 6.55 13.556 7 13 7c-.552 0-1-.443-1-1.01V4.01C12 3.45 12.444 3 13 3zM6 8v7H5V8h1zm9 0v7h-1V8h1z"/>
    </symbol>
    <symbol id="icon-add-app" viewBox="0 0 20 20">
        <path fill="#50B7F4" fill-rule="evenodd" d="M9 9H6v2.017h3V14h2.013v-2.983h2.967V9h-2.967V6H9v3zm1 9c4.418 0 8-3.582 8-8s-3.582-8-8-8-8 3.582-8 8 3.582 8 8 8z"/>
    </symbol>
    <symbol id="icon-get-location" viewBox="0 0 20 20">
        <defs><circle id="a" cx="10" cy="10" r="9"/><mask id="b" width="18" height="18" x="0" y="0" fill="#fff"><use xlink:href="#a"/></mask></defs><g fill="none" fill-rule="evenodd"><use stroke="#FFF" stroke-width="4" mask="url(#b)" xlink:href="#a"/><path fill="#FFF" d="M2 9h3v2H2V9zm13 0h3v2h-3V9z"/><path fill="#FFF" d="M11.033 1.042v4H9v-4h2.033zm0 14V18.5H9v-3.458h2.033z"/></g>
    </symbol>
    <symbol id="icon-reports-big" viewBox="0 0 30 30">
       <path fill="#FFF" fill-rule="evenodd" d="M2 16h6v14H2V16zm10-8h6v22h-6V8zm10-8h6v30h-6V0z" opacity=".5"/>
    </symbol>
    <symbol id="icon-my-deals" viewBox="0 0 30 30">
       <path fill="#FFF" fill-rule="evenodd" d="M9 6H1.992C.898 6 0 6.89 0 7.99v18.02C0 27.11.892 28 1.992 28h26.016C29.102 28 30 27.11 30 26.01V7.99C30 6.89 29.108 6 28.008 6H21c0-2.21-1.79-4-3.996-4h-4.008C10.796 2 9 3.79 9 6zm7.732 10H30v-1H0v1h13.268c-.17.294-.268.636-.268 1 0 1.105.895 2 2 2s2-.895 2-2c0-.364-.097-.706-.268-1zM9 6h3c0-.55.452-1 .996-1h4.008c.548 0 .995.446.996 1h3v.002H9V6z" opacity=".5"/>
    </symbol>
    <symbol id="icon-new-rental" viewBox="0 0 30 30">
       <defs><path id="a" d="M5 3h14.99v4.997H25V27H5z"/><mask id="b" width="20" height="24" x="0" y="0" fill="#fff"><use xlink:href="#a"/></mask></defs><use fill="none" fill-rule="evenodd" stroke="#FFF" stroke-width="4" mask="url(#b)" xlink:href="#a" opacity=".5"/>
    </symbol>
    <symbol id="icon-user" viewBox="0 0 30 30">
       <g fill="none" fill-rule="evenodd"><circle cx="15" cy="15" r="15" fill="#F1F1F1"/><path fill="#9D9D9D" d="M15 14c2.76 0 5-2.24 5-5s-2.24-5-5-5-5 2.24-5 5 2.24 5 5 5zm9.2 8.572c0-5.098-4.167-7.57-9.192-7.57C9.982 15 6 17.472 6 22.57c0 5.098 18.2 5.098 18.2 0z"/></g>
    </symbol>
    <symbol id="icon-checked" viewBox="0 0 30 30">
       <path fill="#626262" fill-rule="evenodd" d="M8.485 15.385L2.828 9.728 0 12.556l8.485 8.486 1.59-1.59L24.043 5.484l-2.83-2.828-12.727 12.73z"/>
    </symbol>
    <symbol id="icon-success" viewBox="0 0 30 30">
       <path fill="#626262" fill-rule="evenodd" d="M8.485 15.385L2.828 9.728 0 12.556l8.485 8.486 1.59-1.59L24.043 5.484l-2.83-2.828-12.727 12.73z"/>
    </symbol>
    <symbol id="icon-get-location" viewBox="0 0 20 20">
       <path fill="#FFF" fill-rule="evenodd" d="M11.033 16.924c3.05-.45 5.46-2.87 5.896-5.924H14V9h2.93c-.438-3.055-2.847-5.473-5.897-5.924v2.966H9v-2.97C5.936 3.508 3.51 5.932 3.07 9H6v2H3.07c.44 3.066 2.865 5.49 5.93 5.93v-2.89h2.033v2.882zM1 10c0-4.97 4.03-9 9-9s9 4.03 9 9-4.03 9-9 9-9-4.03-9-9z"/>
    </symbol>
    <symbol id="icon-reject" viewBox="0 0 40 40">
       <path fill="#DA2E00" fill-rule="evenodd" d="M32.69 10.252C34.764 12.952 36 16.332 36 20c0 8.837-7.163 16-16 16-3.668 0-7.05-1.235-9.748-3.31L32.69 10.25zM29.874 7.41C27.154 5.274 23.724 4 20 4 11.163 4 4 11.163 4 20c0 3.726 1.274 7.155 3.41 9.875L29.875 7.41zM20 40c11.046 0 20-8.954 20-20S31.046 0 20 0 0 8.954 0 20s8.954 20 20 20z"/>
    </symbol>
    <symbol id="icon-search" viewBox="0 0 15 15"><defs><path id="a" d="M14.32 12.906l4.75 4.75-1.413 1.415-4.75-4.75C11.55 15.374 9.847 16 8 16c-4.418 0-8-3.582-8-8s3.582-8 8-8 8 3.582 8 8c0 1.85-.627 3.55-1.68 4.906z"/><mask id="b" width="19.071" height="19.071" x="0" y="0" fill="#fff"><use xlink:href="#a"/></mask></defs><use fill="none" fill-rule="evenodd" stroke="#989897" stroke-width="4" mask="url(#b)" xlink:href="#a"/></symbol>
</svg>
<!--<svg aria-hidden="true" class="icon-svg icon-edit"><use xlink:href="#icon-edit"></use></svg>
<svg aria-hidden="true" class="icon-svg icon-checked"><use xlink:href="#icon-checked"></use></svg>-->
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3 col-lg-2">
                <nav class="navbar navbar-inverse navbar-fixed-side">
                    <div class="navbar-header">
                        <div class="visible-xs-inline-block visible-sm-inline-block visible-md-inline-block">
                        @if (User.Identity.IsAuthenticated)
                            {
                                <button class="navbar-toggle pull-left" data-target=".navbar-collapse" data-toggle="collapse">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            }
                        </div>
                        <div>
                            <div class="dealnet-image-ecologo main-logo">
                            </div>
                        </div>
                        <div>
                            <div class="hidden-md hidden-lg visible-xs-inline-block visible-sm-inline-block dealnet-login-buttons-holder pull-right">
                                @Html.Partial("LoginButtons")
                            </div>
                        </div>
                    </div>
                    @if (User.Identity.IsAuthenticated)
                    {

                    <div class="collapse navbar-collapse" aria-expanded="false">
                        <div class="dealnet-sidebar-header">
                            <div class="dealnet-image-ecologo">
                            </div>
                            <button class="navbar-toggle pull-right" data-target=".navbar-collapse" data-toggle="collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>

                        @if (Request.IsAuthenticated)
                        {
                            {
                            @*@Html.AntiForgeryToken()*@
                                <div class="dealnet-sidebar-user">
                                    <span class="icon-circle-hold"><svg aria-hidden="true" class="icon-svg icon-user"><use xlink:href="#icon-user"></use></svg></span> @User.Identity.Name
                                </div>

                            }
                        }

                        <ul class="nav navbar-nav">
                            <li class="dealnet-sidebar-item" id="sidebar-item-homepage">
                                <div class="dealnet-sidebar-item-inner dealnet-sidebar-image dealnet-image-homepage"></div>
                                <div class="dealnet-sidebar-item-inner">@Html.ActionLink("Home page", "Index", "Home", null, new { @class = "dealnet-link" })</div>
                            </li>
                            <li class="dealnet-sidebar-item" id="sidebar-item-newrental">
                                <div class="dealnet-sidebar-item-inner dealnet-sidebar-image dealnet-image-newrental"></div>
                                <div class="dealnet-sidebar-item-inner">@Html.ActionLink("New rental", "BasicInfo", "NewRental", null, new { @class = "dealnet-link" })</div>
                            </li>
                            <li class="dealnet-sidebar-item" id="sidebar-item-mydeals">
                                <div class="dealnet-sidebar-item-inner dealnet-sidebar-image dealnet-image-mydeals"></div>
                                <div class="dealnet-sidebar-item-inner">@Html.ActionLink("My deals", "MyDeals", "Stub", null, new { @class = "dealnet-link" })</div>
                            </li>
                            <li class="dealnet-sidebar-item" id="sidebar-item-reports">
                                <div class="dealnet-sidebar-item-inner dealnet-sidebar-image dealnet-image-reports"></div>
                                <div class="dealnet-sidebar-item-inner">@Html.ActionLink("Reports", "Reports", "Stub", null, new { @class = "dealnet-link" })</div>
                            </li>
                        </ul>

                        <div class="dealnet-sidebar-logout">
                            <div class="dealnet-sidebar-item">
                                @Html.ActionLink("Logout", "LogOff", "Account", null, new { @class = "dealnet-link" })
                            </div>
                        </div>

                    </div>
                    }
                </nav>
            </div>

            <div class="col-md-9 col-lg-10">
                <div class="row dealnet-page-header visible-md-block visible-lg-block">
                    <div class="col-lg-12 col-md-12  visible-md-block visible-lg-block">
                        @Html.Partial("LoginButtons")
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="@(User.Identity.IsAuthenticated ? "col-md-9 col-lg-10 col-md-offset-3 col-lg-offset-2" : "col-md-12 col-lg-12")">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12 dealnet-body">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="overlay"></div>
    @RenderSection("Scripts", false)
</body>
</html>
