@{
    ViewBag.Title = "OnBoardSuccess";
}
<div class="onboard-success-page align-middle-section">
    <div class="navbar onboard-navbar navbar-fixed-side">
        <div class="navbar-header customer-navbar-header">
            <div class="dealnet-image-ecologo"></div>
        </div>
    </div>
    <div class="align-middle-inner">
        <svg aria-hidden="true" class="icon icon-success">
            <use xlink:href="@Url.Content("~/Content/images/sprite/sprite.svg#icon-success-circle")"></use>
        </svg>
        <h2 class="page-heading">
            @Resources.ApplicationSuccessfullySubmitted<br /> @Resources.WellContactSoon
        </h2>
        <div>
            If you have any questions please contact dealer support:<br />
            @Resources.Phone: @System.Configuration.ConfigurationManager.AppSettings["DealerSupportPhone"]<br />
            @Resources.Email: @System.Configuration.ConfigurationManager.AppSettings["InfoEmail"]
        </div>
    </div>
</div>

