@{
    ViewBag.Title = "OnBoardSuccess";
}
<div class="onboard-success-page align-middle-section">
    <div class="navbar onboard-navbar navbar-fixed-side">
        <div class="navbar-header customer-navbar-header">
            <div class="dealnet-image-ecologo"></div>
        </div>
    </div>
    <div class="align-middle-inner">
        <svg aria-hidden="true" class="icon icon-success">
            <use xlink:href="@Url.Content("~/Content/images/sprite/sprite.svg#icon-success-circle")"></use>
        </svg>
        <h2 class="page-heading">
            @Resources.ApplicationSuccessfullySubmitted<br /> @Resources.WellContactSoon
        </h2>
        <div>
            @Resources.QuestionsContactDealerSupport<br />
            @Resources.Phone:
            <a href="tel:+@System.Configuration.ConfigurationManager.AppSettings["DealerSupportPhone"]" class="tel">
                @System.Configuration.ConfigurationManager.AppSettings["DealerSupportPhone"]
            </a>
            <br />
            @Resources.Email:
            <a href="mailto:@System.Configuration.ConfigurationManager.AppSettings["InfoEmail"]">
                @System.Configuration.ConfigurationManager.AppSettings["InfoEmail"]
            </a>
        </div>
    </div>
</div>
<script type="text/javascript">
	$(document).ready(function () {
		//var salesrep = $('#OnBoardingLink').val();
		gtag('event', 'Dealer Application Submit', { 'event_category': 'Dealer Application Submit', 'event_action': 'Submit button clicked', 'event_label': 'Successfull' });
	});

</script>

