@{
    ViewBag.Title = Resources.Onboarding + " " + Resources.Error;
}
<div class="onboard-success-page">
    <div class="navbar onboard-navbar navbar-fixed-side">
        <div class="navbar-header customer-navbar-header">
            <div class="dealnet-image-ecologo"></div>
        </div>
    </div>
    <div class="align-middle-inner">
        <svg aria-hidden="true" class="icon icon-error">
            <use xlink:href="@Url.Content("~/Content/images/sprite/sprite.svg#icon-error")"></use>
        </svg>
        <h2 class="page-heading">
            @Resources.SomethingWentWrong <br /> @Resources.CheckYourLink
        </h2>
        <div>
            @Resources.QuestionsContactDealerSupport
            <br />
            @Resources.Phone:
	        <a href="tel:+@Resources.DealerSupportPhoneNumber" class="tel">
		        @Resources.DealerSupportPhoneNumber
	        </a>
            <br />
            @Resources.Email: <a href="mailto: @Resources.DealerSupportInfoEmail">
	        @Resources.DealerSupportInfoEmail
	        </a>
        </div>
    </div>
</div>

<script type="text/javascript">
	$(document).ready(function () {
		gtag('event', 'Dealer Application Submit', { 'event_category': 'Dealer Application Submit', 'event_action': 'Submit button clicked', 'event_label': 'Error' });
	});

</script>
