<% layout('_layouts/default') -%>
<% title = "DealNet home" -%>
<div class="custom-container">
  <a class="main-banner" href="#" style="background-image: url('../img/banner.png');"></a>
</div>
<div class="container-fluid custom-container">
  <div class="margin-block">
    <button type="button" class="btn btn-empty-grey min-w-100">Clear</button>

    <button type="button" class="btn btn-empty-green min-w-100">Save</button>

    <button type="button" class="btn btn-empty-red min-w-100">Filter</button>

    <button type="button" class="btn btn-red">Apply</button>

    <button type="button" class="btn btn-green min-w-60">Next</button>
  </div>
  <div class="margin-block">
    <button type="button" class="btn btn-empty-grey min-w-100 disabled">Clear</button>

    <button type="button" class="btn btn-empty-green min-w-100 disabled">Save</button>

    <button type="button" class="btn btn-empty-red min-w-100 disabled">Filter</button>

    <button type="button" class="btn btn-red disabled">Apply</button>

    <button type="button" class="btn btn-green min-w-60 disabled">Next</button>
  </div>
  <div class="margin-block">
    <div class="row">
      <div class="col-xs-2">
        <form action="#">
          <div class="custom-text-input-group">
            <input type="text" required value="$ 12,400.00">
            <label>Cost</label>
          </div>
        </form>
      </div>
      <div class="col-xs-3">
        <div class="custom-text-input-group">
          <input type="text" required>
          <label>Email</label>
        </div>
      </div>
      <div class="col-xs-4">
        <div class="custom-text-input-group">
          <input type="text" required>
          <label>Email</label>
        </div>
      </div>
    </div>
  </div>
  <div class="margin-block">
    <div class="row">
      <div class="col-xs-2">
        <form action="#">
          <div class="custom-text-input-group error">
            <input type="text" required>
            <label>Name</label>
            <div class="error-message">
              <span>Cost is in incorrect format</span>
            </div>
          </div>
        </form>
      </div>
      <div class="col-xs-3">
        <div class="custom-text-input-group error">
          <input type="text" required>
          <label>Description</label>
          <div class="error-message">
            <span>This field canâ€™t be empty</span>
          </div>
        </div>
      </div>
      <div class="col-xs-4">
        <div class="custom-text-input-group error">
          <input type="text" required>
          <label>Dealer's Initial</label>
          <div class="error-message">
            <span>The field Dealer's Initial must be a string with a minimum length of 1 and a maximum length of 2.</span>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="margin-block">
    <div class="action-link blue">
      <span class="icon-add-app"></span>
      Add additional applicant
    </div>
    <div class="action-link grey">
      <span class="icon-delete-name"></span>
      Clear Address
    </div>
    <div class="clearfix"></div>
    <div class="action-link blue disabled">
      <span class="icon-add-app"></span>
      Add additional applicant
    </div>
    <div class="action-link grey disabled">
      <span class="icon-delete-name"></span>
      Clear Address
    </div>
    <div class="clearfix"></div>
  </div>
  <div class="margin-block">
    <label class="checkbox-container">Borrower lived at current address less than 6 month
      <input type="checkbox" checked="checked">
      <span class="checkmark"></span>
    </label>
  </div>
  <div class="margin-block">
    <div class="row">
      <div class="col-xs-3">
        <div class="custom-select-wrap">
          <label for="project-type">Type of project</label>
          <select name="project-type" id="project-type" class="custom-select">
            <option value="">Air conditioner</option>
            <option value="">Air filtration</option>
            <option value="">Air handler</option>
          </select>
        </div>
      </div>
    </div>
  </div>
  <div class="margin-block">
    <div class="row">
      <div class="col-xs-3">
        <div class="custom-text-input-group icon">
          <input type="text" required>
          <label>Date from</label>
          <span class="icon-calendar"></span>
        </div>
      </div>
    </div>
  </div>
  <div class="pagination custom-pagination">
    <nav aria-label="Page navigation">
      <ul class="pagination">
        <li>
          <a href="#" aria-label="Previous">
            <span aria-hidden="true" class="icon-pagination-left"></span>
          </a>
        </li>
        <li class="active"><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">...</a></li>
        <li><a href="#">12</a></li>
        <li>
          <a href="#" aria-label="Next">
            <span aria-hidden="true" class="icon-pagination-right"></span>
          </a>
        </li>
      </ul>
    </nav>
  </div>
</div>
<div class="help-cloud">
  <span class="icon-help-button"></span>
  Help
</div>
